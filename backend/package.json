{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@tsed/ajv": "^6.62.0",
    "@tsed/common": "^6.62.0",
    "@tsed/core": "^6.62.0",
    "@tsed/di": "^6.62.0",
    "@tsed/exceptions": "^6.62.0",
    "@tsed/json-mapper": "^6.62.0",
    "@tsed/passport": "^6.62.0",
    "@tsed/platform-express": "^6.62.0",
    "@tsed/schema": "^6.62.0",
    "@tsed/socketio": "^6.62.0",
    "ajv": "^8.6.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "passport": "^0.4.1",
    "sequelize": "^6.6.5",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
