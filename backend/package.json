    "@types/express": "^4.17.13",
    "@types/method-override": "0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.8",
    "@types/passport": "^1.0.7",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "tsed": {
    "packageManager": "npm",
    "convention": "default"
  }
}
